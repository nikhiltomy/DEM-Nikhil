    <mj-section>
    	<mj-column>
      <mj-button  text-transform="uppercase" padding="20px 0px 10px" href="https://www.sephora.it/YSL-BrandUniverse-4.html  " background-color="#fff" border-radius="0px" color="#000" font-weight="800" inner-padding="20px 60px" font-size="16px"><span class="font-change3">SCOPRI DI PIÙ
</span></mj-button>
        
      </mj-column>
    </mj-section>
    
    <mj-section>
    	<mj-column>
      <mj-image src="img/image01.png" width="505px" padding="20px 30px 0px 30px" ></mj-image>
