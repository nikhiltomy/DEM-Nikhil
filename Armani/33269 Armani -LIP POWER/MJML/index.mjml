<mjml>
  <mj-head>
    <mj-preview>Vieni a trovarci in Rinascente Duomo per una splendida sorpresa</mj-preview>
    <mj-attributes>
			<mj-all padding="0px" font-family="Arial, sans-serif"></mj-all>
      <mj-class name="centered" align="center"></mj-class>
  		<mj-class name="blacked" color="#000000"></mj-class>
      <mj-class name="whited" color="#ffffff"></mj-class>
      <mj-class name="bolded" font-weight="bold"></mj-class>
    </mj-attributes>
    
    <mj-style>
    	 .link-nonstyle{
      color: inherit; text-decoration: none !important;
    	}
      a:link{
      color: inherit; text-decoration: none;
      }	
      a:visited{
      color:inherit; text-decoration: none;
      }
      .no-underline { text-decoration: none !important; }

      
      @media only screen and (max-width: 480px) {
        .hidden-mobile {
          display: none !important;
        }

        .display-mobile {
          display: block !important;
          max-height: none !important;
        }
      .padding-change1 td{
      padding:10px 0px 0px 0px !important;
      }
      }
      
      @media only screen and (min-width: 481px) {
        .hidden-desktop {
          display: none !important;
        }

        .display-desktop {
          display: block !important;
          max-height: none !important;
        }
  
      }
    </mj-style>

  </mj-head>

